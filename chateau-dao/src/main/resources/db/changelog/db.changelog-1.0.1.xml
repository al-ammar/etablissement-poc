<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet id="create-user-table" author="alaa">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="ETABLISSEMENT"/>
            </not>
        </preConditions>
        <createTable tableName="ETABLISSEMENT">
            <column name="ID" type="varchar2(40)">
                <constraints primaryKey="true" nullable="false"/>
            </column>
			<column name="CODE" type="varchar2(100)"/>
            <column name="LIBELLE" type="varchar2(100)"/>
			<column name="DESCRIPTION" type="varchar2(255)"/>
            <column name="ADRESSE" type="varchar2(255)">
            </column>
            <column name="TELEPHONE" type="varchar2(50)">
            </column>
            <column name="ETAT" type="varchar2(50)">
            </column>
            <column name="INSERTED_AT" type="timestamp(6)"/>
            <column name="INSERTED_BY" type="varchar2(255)"/>
            <column name="UPDATED_AT" type="timestamp(6)"/>
            <column name="UPDATED_BY" type="varchar2(255)"/>
			<column name="UPDATE_DATE" type="timestamp(6)"/>

        </createTable>
        <addUniqueConstraint tableName="ETABLISSEMENT" constraintName="UDX_ETABLISSEMENT"
                             columnNames="ID"/>
    </changeSet>

</databaseChangeLog>